{
  "name": "discord-rpc-new",
  "type": "module",
  "version": "1.3.2",
  "description": "A lightweight, typed Discord RPC client",
  "keywords": [
    "discord",
    "rpc",
    "rich presence",
    "remote procedural call"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CuteNikki/discord-rpc-new.git"
  },
  "bugs": {
    "url": "https://github.com/CuteNikki/discord-rpc-new/issues"
  },
  "homepage": "https://github.com/CuteNikki/discord-rpc-new#readme",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "types": "./dist/index.d.mts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*.d.mts",
        "./*"
      ]
    }
  },
  "scripts": {
    "test": "bun ./playground.ts",
    "build": "rm -rf dist && tsdown",
    "prepublish": "bun run build"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "bun-types": "^1.3.9",
    "tsdown": "^0.20.3"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  }
}
